{
  "name": "node.utils",
  "version": "0.1.0",
  "description": "Template for NodeJS",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc --build --clean && tsc --build && npm run .copy-dist",
    "lint": "eslint ./src/**/*.ts && tslint ./src/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts && tslint --fix ./src/**/*.ts",
    "test": "jest",
    ".. sub script ..": "",
    ".copy-dist": "cpx package.json dist/ && cpx LICENSE dist/ && cpx README.md dist/",
    "__ memo __": "",
    "__versions:installed__": "npm ls --depth=0",
    "__outdated__": "npm outdated"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "cpx": "^1.5.0",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.0.4",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpler-one/node.utils.git"
  },
  "keywords": [
    "util"
  ],
  "author": "Hiroaki Kobayashi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simpler-one/node.utils/issues"
  },
  "homepage": "https://github.com/simpler-one/node.utils#readme"
}
