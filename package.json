{
  "name": "node.utils",
  "version": "0.1.0",
  "description": "Template for NodeJS",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p src && npm run .copy-dist",
    "lint": "eslint ./src/**/*.ts && tslint ./src/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts && tslint --fix ./src/**/*.ts",
    "test": "rimraf spec/built && tsc -p spec && istanbul cover node_modules/jasmine/bin/jasmine.js -x \"**/*.spec.js\"",
    ".copy-dist": "cpx package.json dist/ && cpx LICENSE dist/ && cpx README.md dist/"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^3.3.13",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "codecov": "^3.5.0",
    "cpx": "^1.5.0",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "jasmine": "^3.4.0",
    "remap-istanbul": "^0.13.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typescript": "^3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpler-one/node.utils.git"
  },
  "keywords": [
    "util"
  ],
  "author": "Hiroaki Kobayashi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simpler-one/node.utils/issues"
  },
  "homepage": "https://github.com/simpler-one/node.utils#readme"
}
